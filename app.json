      "bundleIdentifier": "com.yasir2002.weather"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pavitra_behara.weather"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "42cee087-269b-47e6-925c-54c5c84aa72b"
      }
    }
  }
}
